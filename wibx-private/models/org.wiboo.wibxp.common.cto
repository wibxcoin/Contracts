namespace org.wiboo.wibxp.common

/**
 * WiBOO User Wallet
 */
participant Wallet identified by idWallet
{
    /**
     * Wallet identification
     */
    o String idWallet

    /**
     * The ETH address
     */
    o String ethAddress

    /**
     * Coin balance of the wallet
     */
    o Double balance
}